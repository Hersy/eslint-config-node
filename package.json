{
  "name": "@hersy/eslint-config-node",
  "version": "0.5.0",
  "description": "A modern ESLint configuration for NodeJS, following my styleguide",
  "keywords": [
    "config",
    "hersy",
    "eslint",
    "eslintconfig",
    "javascript",
    "typescript",
    "node",
    "styleguide"
  ],
  "homepage": "https://github.com/hersy/eslint-config-node",
  "bugs": {
    "url": "https://github.com/hersy/eslint-config-node/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hersy/eslint-config-node"
  },
  "license": "MIT",
  "author": "Hersy <contact@hersy.dev>",
  "type": "module",
  "files": [
    "configs",
    "index.js"
  ],
  "main": "index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "lint": "eslint . --report-unused-disable-directives --ext .js,json,md",
    "lint-fix": "eslint . --fix --ext .js,json,md",
    "format": "yarn run --silent lint-fix",
    "eslint-check": "eslint --print-config eslint.config.js",
    "pretests-only": "node ./test/imports",
    "pretest": "yarn run --silent lint",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "test": "yarn run --silent tests-only",
    "prepublishOnly": "eslint-find-rules --unused ./index.js --flat-config && yarn test",
    "prepublish": "not-in-publish || yarn run prepublishOnly"
  },
  "dependencies": {
    "@eslint/css": "^0.7.0",
    "@eslint/js": "^9.24.0",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.4.0",
    "@html-eslint/eslint-plugin": "^0.39.0",
    "@html-eslint/parser": "^0.39.0",
    "@microsoft/eslint-plugin-sdl": "^1.1.0",
    "@stylistic/eslint-plugin": "^4.2.0",
    "eslint-flat-config-utils": "^2.0.0",
    "eslint-import-resolver-typescript": "^4.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-name": "^1.2.0",
    "eslint-plugin-jsdoc": "^50.6.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-promise": "^7.2.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-security": "^3.0.0",
    "eslint-plugin-sonarjs": "^3.0.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-unused-imports": "^4.1.0",
    "globals": "^16.0.0",
    "typescript-eslint": "^8.30.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/runtime": "^7.27.0",
    "@hersy/babel-preset": "^0.2.0",
    "@hersy/babel-tape-runner": "^1.0.0",
    "eslint": "^9.24.0",
    "eslint-find-rules": "^5.0.0",
    "in-publish": "^2.0.0",
    "rimraf": "^6.0.0",
    "tape": "^5.9.0",
    "typescript": "^5.8.0"
  },
  "peerDependencies": {
    "eslint": "^9.24.0"
  },
  "resolutions": {
    "glob": "^9.3.0"
  },
  "packageManager": "yarn@1.22.22"
}